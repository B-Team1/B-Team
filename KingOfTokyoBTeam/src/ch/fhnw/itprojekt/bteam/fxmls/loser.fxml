<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="400.0" prefWidth="458.0" styleClass="root" stylesheets="@meincss.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.fhnw.itprojekt.bteam.appClasses.LoseController">
   <center>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <VBox alignment="CENTER" layoutY="4.0" spacing="20.0" AnchorPane.leftAnchor="25.0" AnchorPane.rightAnchor="25.0" AnchorPane.topAnchor="20.0">
               <children>
                  <Label layoutX="83.0" layoutY="4.0" text="%loser.lb.loser">
                     <font>
                        <Font size="36.0" />
                     </font>
                  </Label>
                  <ImageView fitHeight="188.0" fitWidth="197.0" layoutY="25.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/loser.png" />
                     </image>
                  </ImageView>
                  <Button fx:id="btnQuitGame" layoutX="68.0" layoutY="160.0" mnemonicParsing="false" onAction="#handleQuitGame" prefHeight="35.0" prefWidth="100.0" stylesheets="@meincss.css" text="%loser.btn.close" />
               </children>
            </VBox>
         </children>
      </AnchorPane>
   </center>
